  <xs:element name="Ellipse">
    <xs:annotation>
      <xs:documentation>Draws an ellipse.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:complexContent>
        <xs:extension base="FrameworkElement">
          <xs:attribute name="RenderedGeometry" type="text">
            <xs:annotation>
              <xs:documentation>Gets the final rendered &lt;see cref="T:System.Windows.Media.Geometry" /&gt; of an &lt;see cref="T:System.Windows.Shapes.Ellipse" /&gt;.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="GeometryTransform" type="text">
            <xs:annotation>
              <xs:documentation>Gets the value of any &lt;see cref="P:System.Windows.Media.Transform.Identity" /&gt; transforms that are applied to the &lt;see cref="T:System.Windows.Media.Geometry" /&gt; of an &lt;see cref="T:System.Windows.Shapes.Ellipse" /&gt; before it is rendered.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="Stretch" type="text" />
          <xs:attribute name="Fill" type="text" />
          <xs:attribute name="Stroke" type="text" />
          <xs:attribute name="StrokeThickness" type="text" />
          <xs:attribute name="StrokeStartLineCap" type="text" />
          <xs:attribute name="StrokeEndLineCap" type="text" />
          <xs:attribute name="StrokeDashCap" type="text" />
