  <xs:element name="Window">
    <xs:annotation>
      <xs:documentation>Provides the ability to create, configure, show, and manage the lifetime of windows and dialog boxes.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:complexContent>
        <xs:extension base="FrameworkElement">
          <xs:attribute name="TaskbarItemInfo" type="text">
            <xs:annotation>
              <xs:documentation>Gets or sets the Windows 7 taskbar thumbnail for the &lt;see cref="T:System.Windows.Window" /&gt;.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="AllowsTransparency" type="text">
            <xs:annotation>
              <xs:documentation>Gets or sets a value that indicates whether a window's client area supports transparency.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="Title" type="text">
            <xs:annotation>
              <xs:documentation>Gets or sets a window's title.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="Icon" type="text">
            <xs:annotation>
              <xs:documentation>Gets or sets a window's icon.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="SizeToContent" type="text">
            <xs:annotation>
              <xs:documentation>Gets or sets a value that indicates whether a window will automatically size itself to fit the size of its content.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="Top" type="text">
            <xs:annotation>
              <xs:documentation>Gets or sets the position of the window's top edge, in relation to the desktop.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="Left" type="text">
            <xs:annotation>
              <xs:documentation>Gets or sets the position of the window's left edge, in relation to the desktop.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="RestoreBounds" type="text">
            <xs:annotation>
              <xs:documentation>Gets the size and location of a window before being either minimized or maximized.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="WindowStartupLocation" type="text">
            <xs:annotation>
              <xs:documentation>Gets or sets the position of the window when first shown.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="ShowInTaskbar" type="text">
            <xs:annotation>
              <xs:documentation>Gets or sets a value that indicates whether the window has a task bar button.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="IsActive" type="text">
            <xs:annotation>
              <xs:documentation>Gets a value that indicates whether the window is active.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="Owner" type="text">
            <xs:annotation>
              <xs:documentation>Gets or sets the &lt;see cref="T:System.Windows.Window" /&gt; that owns this &lt;see cref="T:System.Windows.Window" /&gt;.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="OwnedWindows" type="text">
            <xs:annotation>
              <xs:documentation>Gets a collection of windows for which this window is the owner.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="DialogResult" type="text">
            <xs:annotation>
              <xs:documentation>Gets or sets the dialog result value, which is the value that is returned from the &lt;see cref="M:System.Windows.Window.ShowDialog" /&gt; method.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="WindowStyle" type="text">
            <xs:annotation>
              <xs:documentation>Gets or sets a window's border style.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="WindowState" type="text">
            <xs:annotation>
              <xs:documentation>Gets or sets a value that indicates whether a window is restored, minimized, or maximized.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="ResizeMode" type="text">
            <xs:annotation>
              <xs:documentation>Gets or sets the resize mode.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="Topmost" type="text">
            <xs:annotation>
              <xs:documentation>Gets or sets a value that indicates whether a window appears in the topmost z-order.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="ShowActivated" type="text">
            <xs:annotation>
              <xs:documentation>Gets or sets a value that indicates whether a window is activated when first shown.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
