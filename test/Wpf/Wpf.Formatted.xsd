<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="WpfXamlSchema" targetNamespace="http://schemas.microsoft.com/winfx/2006/xaml/presentation" elementFormDefault="qualified" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:simpleType name="text">
    <xs:restriction base="xs:string" />
  </xs:simpleType>
  <xs:complexType name="FrameworkElement" mixed="true">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:group ref="controls" />
      <xs:any maxOccurs="unbounded" processContents="lax" />
    </xs:choice>
    <xs:attribute name="Style" type="text">
      <xs:annotation>
        <xs:documentation>Source is from a style setter of a non-theme style.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="OverridesDefaultStyle" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets a value indicating whether this element incorporates style properties from theme styles.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="UseLayoutRounding" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets a value that indicates whether layout rounding should be applied to this element's size and position during layout.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Triggers" type="text">
      <xs:annotation>
        <xs:documentation>Gets a collection of &lt;see cref="T:System.Windows.TriggerBase" /&gt; objects that apply property changes or perform actions based on specified conditions.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="TemplatedParent" type="text">
      <xs:annotation>
        <xs:documentation>Gets a static value that is used to return a &lt;see cref="T:System.Windows.Data.RelativeSource" /&gt; constructed for the &lt;see cref="F:System.Windows.Data.RelativeSourceMode.TemplatedParent" /&gt; mode.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Resources" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets a collection of application-scope resources, such as styles and brushes.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="DataContext" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets the data context for an element when it participates in data binding.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="BindingGroup" type="text">
      <xs:annotation>
        <xs:documentation>Gets the &lt;see cref="T:System.Windows.Data.BindingGroup" /&gt; that this binding expression belongs to.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Language" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets localization/globalization language information that applies to an individual element.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Name" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets a name for this &lt;see cref="T:System.Windows.Annotations.AnnotationResource" /&gt;.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Tag" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets an object associated with the dialog. This provides the ability to attach an arbitrary object to the dialog.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="InputScope" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets the context for input used by this &lt;see cref="T:System.Windows.FrameworkContentElement" /&gt;.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ActualWidth" type="text">
      <xs:annotation>
        <xs:documentation>Gets a value that represents the actual calculated width of a &lt;see cref="T:System.Windows.Controls.ColumnDefinition" /&gt;.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ActualHeight" type="text">
      <xs:annotation>
        <xs:documentation>Gets a value that represents the calculated height of the &lt;see cref="T:System.Windows.Controls.RowDefinition" /&gt;.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="LayoutTransform" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets a graphics transformation that should apply to this element when layout is performed.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Width" type="text">
      <xs:annotation>
        <xs:documentation>Gets the calculated width of a &lt;see cref="T:System.Windows.Controls.ColumnDefinition" /&gt; element, or sets the &lt;see cref="T:System.Windows.GridLength" /&gt; value of a column that is defined by the &lt;see cref="T:System.Windows.Controls.ColumnDefinition" /&gt;.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="MinWidth" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets a value that represents the minimum width of a &lt;see cref="T:System.Windows.Controls.ColumnDefinition" /&gt;.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="MaxWidth" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets a value that represents the maximum width of a &lt;see cref="T:System.Windows.Controls.ColumnDefinition" /&gt;.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Height" type="text">
      <xs:annotation>
        <xs:documentation>Gets the calculated height of a &lt;see cref="T:System.Windows.Controls.RowDefinition" /&gt; element, or sets the &lt;see cref="T:System.Windows.GridLength" /&gt; value of a row that is defined by the &lt;see cref="T:System.Windows.Controls.RowDefinition" /&gt;.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="MinHeight" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets a value that represents the minimum allowable height of a &lt;see cref="T:System.Windows.Controls.RowDefinition" /&gt;.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="MaxHeight" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets a value that represents the maximum height of a &lt;see cref="T:System.Windows.Controls.RowDefinition" /&gt;.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="FlowDirection" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets the relative direction for flow of content within a &lt;see cref="T:System.Windows.Documents.Block" /&gt; element.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Margin" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets the margin thickness for the element.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="HorizontalAlignment" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets a value that indicates the horizontal alignment for a &lt;see cref="T:System.Windows.Documents.Floater" /&gt; object.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="VerticalAlignment" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets the vertical alignment characteristics applied to this element when it is composed within a parent element such as a panel or items control.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="FocusVisualStyle" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets an object that enables customization of appearance, effects, or other style characteristics that will apply to this element when it captures keyboard focus.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Cursor" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets the cursor that displays when the mouse pointer is over this element.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ForceCursor" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets a value indicating whether this &lt;see cref="T:System.Windows.FrameworkContentElement" /&gt; should force the user interface (UI) to render the cursor as declared by this instance's &lt;see cref="P:System.Windows.FrameworkContentElement.Cursor" /&gt; property.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="IsInitialized" type="text">
      <xs:annotation>
        <xs:documentation>Gets a value indicating whether this element has been initialized, either by being loaded as Extensible Application Markup Language (XAML), or by explicitly having its &lt;see cref="M:System.Windows.FrameworkContentElement.EndInit" /&gt; method called.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="IsLoaded" type="text">
      <xs:annotation>
        <xs:documentation>Gets a value indicating whether this element has been loaded for presentation.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ToolTip" type="text">
      <xs:annotation>
        <xs:documentation>Represents a control that creates a pop-up window that displays information for an element in the interface.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ContextMenu" type="text">
      <xs:annotation>
        <xs:documentation>Represents a pop-up menu that enables a control to expose functionality that is specific to the context of the control.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Parent" type="text">
      <xs:annotation>
        <xs:documentation>Gets the parent of the collection to register.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="HasAnimatedProperties" type="text" />
    <xs:attribute name="InputBindings" type="text" />
    <xs:attribute name="CommandBindings" type="text" />
    <xs:attribute name="AllowDrop" type="text" />
    <xs:attribute name="DesiredSize" type="text" />
    <xs:attribute name="IsMeasureValid" type="text" />
    <xs:attribute name="IsArrangeValid" type="text" />
    <xs:attribute name="RenderSize" type="text" />
    <xs:attribute name="RenderTransform" type="text" />
    <xs:attribute name="RenderTransformOrigin" type="text" />
    <xs:attribute name="IsMouseDirectlyOver" type="text" />
    <xs:attribute name="IsMouseOver" type="text" />
    <xs:attribute name="IsStylusOver" type="text" />
    <xs:attribute name="IsKeyboardFocusWithin" type="text" />
    <xs:attribute name="IsMouseCaptured" type="text" />
    <xs:attribute name="IsMouseCaptureWithin" type="text" />
    <xs:attribute name="IsStylusDirectlyOver" type="text" />
    <xs:attribute name="IsStylusCaptured" type="text" />
    <xs:attribute name="IsStylusCaptureWithin" type="text" />
    <xs:attribute name="IsKeyboardFocused" type="text" />
    <xs:attribute name="IsInputMethodEnabled" type="text" />
    <xs:attribute name="Opacity" type="text" />
    <xs:attribute name="OpacityMask" type="text" />
    <xs:attribute name="BitmapEffect" type="text" />
    <xs:attribute name="Effect" type="text" />
    <xs:attribute name="BitmapEffectInput" type="text" />
    <xs:attribute name="CacheMode" type="text" />
    <xs:attribute name="Uid" type="text">
      <xs:annotation>
        <xs:documentation>Gets the Uid component of this &lt;see cref="T:System.Windows.Markup.Localizer.BamlLocalizableResourceKey" /&gt;.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Visibility" type="text">
      <xs:annotation>
        <xs:documentation>Gets or sets the visibility of the column.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ClipToBounds" type="text" />
    <xs:attribute name="Clip" type="text" />
    <xs:attribute name="SnapsToDevicePixels" type="text" />
    <xs:attribute name="IsFocused" type="text" />
    <xs:attribute name="IsEnabled" type="text">
      <xs:annotation>
        <xs:documentation>Gets a value that indicates whether the &lt;see cref="T:System.Windows.Annotations.AnnotationService" /&gt; is enabled.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="IsHitTestVisible" type="text" />
    <xs:attribute name="IsVisible" type="text" />
    <xs:attribute name="Focusable" type="text" />
    <xs:attribute name="PersistId" type="text" />
    <xs:attribute name="IsManipulationEnabled" type="text" />
    <xs:attribute name="AreAnyTouchesOver" type="text" />
    <xs:attribute name="AreAnyTouchesDirectlyOver" type="text" />
    <xs:attribute name="AreAnyTouchesCapturedWithin" type="text" />
    <xs:attribute name="AreAnyTouchesCaptured" type="text" />
    <xs:attribute name="TouchesCaptured" type="text" />
    <xs:attribute name="TouchesCapturedWithin" type="text" />
    <xs:attribute name="TouchesOver" type="text" />
    <xs:attribute name="TouchesDirectlyOver" type="text" />
    <xs:attribute name="DependencyObjectType" type="text" />
    <xs:attribute name="IsSealed" type="text">
      <xs:annotation>
        <xs:documentation>Gets a value that indicates whether this trigger is read-only and cannot be changed .</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Dispatcher" type="text" />
    <xs:anyAttribute namespace="##local http://schemas.microsoft.com/winfx/2006/xaml http://schemas.openxmlformats.org/markup-compatibility/2006" processContents="skip" />
  </xs:complexType>
